@{
    ViewData["Title"] = "Redirect";
    Layout = "~/Views/Shared/_LoginLayout.cshtml";


}


<link href="~/css/signin.css" rel="stylesheet">

<br />
<br />
<br />

<body class="text-center" onLoad="redireccionar()">

    <img class="mb-4" src="~/img/logo.png" alt="" width="72" height="72">
    <div class="alert alert-danger" role="alert">
        <h4 class="alert-heading">No tienes Permiso</h4>
        <p class="mb-0">No tienes permiso de estar aqui, primero debes iniciar sesion <a asp-action="Index">Login</a></p>
    </div>
    <p class="mt-5 mb-3 text-muted">&copy; 2020-2020</p>

</body>
<script language="JavaScript">
    function redireccionar() {
        setTimeout("location.href='../'", 5000);
    }
</script>